<fieldset class="imageupload">
    <div class="col-sm-12">
        <label class="control-label">Multi Fileupload:</label>
        <iframe src="{{ paths.root }}imageupload/iframe/{{ context.contenttype.singular_slug }}/{{ key }}" style="width:100%;height:700px;border:none"></iframe>
        <input style="display:none" class="form-control large" name="{{ key }}" id="imageupload" value="{{ context.content.get(contentkey) }}" type="text">
        <iframe src="{{ paths.root }}bolt/imageupload/iframe/{{ context.contenttype.singular_slug }}/{{ key }}" style="width:100%;height:700px;border:none"></iframe>
        <input style="display:none" class="form-control large" name="{{ key }}" id="imageupload" value="{{ context.content.get(contentkey) }}" type="text">
    </div>
</fieldset>
